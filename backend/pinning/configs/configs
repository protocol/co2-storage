[postgresql]
postgresql_host = localhost
postgresql_port = 5432
postgresql_database = co2_storage
postgresql_user = co2_storage
postgresql_password = DATABASE_PASSWORD

[log]
logfile = logs/log

[scheduler]
try_pinning_every = @every 30s
check_pinning_nodes = @every 1h
calculate_pin_size = @every 1m

[ipfs]
ipfs_node = /ip4/127.0.0.1/tcp/5001
